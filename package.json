{
  "name": "liquipack-systems-f-end",
  "version": "1.8.3",
  "scripts": {
    "ng": "ng",
    "start": "npm run version:info:gen && ng serve --host 0.0.0.0 --disableHostCheck --proxy-config proxy.conf.json",
    "build_prod": "npm run version:info:gen && ng build --prod --aot=false --build-optimizer=false --baseHref=/ --output-path=./dist/build_prod/",
    "build_aot": "npm run version:info:gen && ng build --baseHref=/ --build-optimizer --aot --output-path=./dist/build_aot/",
    "build_aot_stats": "npm run version:info:gen && ng build --baseHref=/ --build-optimizer --aot --output-path=./dist/build_aot/ --stats-json",
    "build_aot_gzip": "npm run version:info:gen && ng build --baseHref=/ --build-optimizer --aot --output-path=./dist/build_aot_gzip/ && gzipper --verbose --gzip-level 9 ./dist/build_aot_gzip",
    "build_prod_aot_gzip": "npm run version:info:gen && ng build --baseHref=/ --prod --output-path=./dist/ && gzipper --verbose --gzip-level 9 ./dist",
    "prepare_nginx": "sudo cp -rfv ./nginx/default /etc/nginx/sites-enabled && sudo systemctl restart nginx",
    "prepare_ng_app": "npm run build_prod_aot_gzip && sudo cp -rfv ./dist/* /var/www/html",
    "deploy_aot_gzip": "npm run prepare_nginx && npm run prepare_ng_app",
    "analyze_es5": "webpack-bundle-analyzer ./dist/build_aot/stats-es5.json",
    "analyze_es2015": "webpack-bundle-analyzer ./dist/build_aot/stats-es2015.json",
    "test": "npm run version:info:gen && ng test",
    "test:CI": "npm run version:info:gen && ng test --browsers ChromeHeadless --no-watch --code-coverage",
    "e2e": "ng e2e",
    "lint:fix": "ng lint liquipack-systems-f-end --fix",
    "lint:check": "ng lint",
    "format": "prettier \"src/**/*.{css,html,js,json,md,scss,ts}\"",
    "format:fix": "prettier --write \"src/**/*.{css,html,js,json,md,scss,ts}\"",
    "format:check": "prettier -c \"src/**/*.{css,html,js,json,md,scss,ts}\"",
    "version:info:gen": "node version-info-gen.ts",
    "firebase:target:apply:prod": "firebase target:apply hosting prod liquipack",
    "firebase:target:apply:testbed": "firebase target:apply hosting testbed testbed-liquipack",
    "firebase:deploy:prod": "firebase deploy --only hosting:prod",
    "firebase:deploy:testbed": "firebase deploy --only hosting:testbed"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.13",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.13",
    "@angular/compiler": "^8.2.13",
    "@angular/core": "^8.2.13",
    "@angular/forms": "^8.2.13",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.13",
    "@angular/platform-browser-dynamic": "^8.2.13",
    "@angular/router": "^8.2.13",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@types/jquery": "^3.3.31",
    "angular-tslint-rules": "^1.20.1",
    "core-js": "^2.6.10",
    "fast-deep-equal": "^2.0.1",
    "fs-extra": "^8.1.0",
    "git-describe": "^4.0.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "rgb-hex": "^3.0.0",
    "rxjs": "^6.5.3",
    "ts-md5": "^1.2.7",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.6",
    "@angular/cli": "^8.3.17",
    "@angular/compiler-cli": "^8.2.13",
    "@angular/language-service": "^8.2.13",
    "@types/fs-extra": "^8.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.0",
    "gzipper": "^2.10.4",
    "husky": "^3.0.9",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "1.18.2",
    "prettier-tslint": "^0.4.2",
    "pretty-quick": "^1.11.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.4.5",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
